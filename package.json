{
  "name": "@stormstreaming/stormplayer-react",
  "version": "2.0.0-beta.0",
  "description": "Dedicated StormPlayer component for React.js",
  "private": false,
  "author": "Web-Anatomy s.c. <contact@web-anatomy.com>",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "*"
  ],
  "license": "SEE LICENSE IN LICENSE.txt",
  "repository": {
    "type": "git",
    "url": "https://github.com/StormStreaming/stormplayer-react"
  },
  "homepage": "https://github.com/StormStreaming/stormlibrary-react#readme",
  "keywords": [
    "stormstreaming",
    "web",
    "video",
    "player",
    "library",
    "react",
    "low-latency",
    "live-streaming"
  ],
  "bugs": {
    "url": "https://github.com/StormStreaming/stormplayer-react/issues"
  },
  "scripts": {
    "clear": "rm -rf dist",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "lint": "yarn eslint ./src",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "code-check": "yarn typecheck && yarn test && yarn lint",
    "pre-:dev": "yarn clear && cp ./src/index.dev.tsx ./src/index.tsx",
    "pre-:prod": "yarn clear && cp ./src/index.prod.ts ./src/index.tsx",
    "dev": "yarn run pre-:dev && cross-env NODE_ENV=development rollup -c -w",
    "prepublishOnly": "yarn build",
    "build:types": "yarn run tsc --project tsconfig.json && yarn tscpaths -p tsconfig.json -s ./src -o ./dist/types",
    "build": "yarn pre-:prod && yarn build:types && cross-env NODE_ENV=production rollup -c"
  },
  "dependencies": {
    "@date-io/moment": "^2.16.1",
    "@material-ui/core": "^4.12.4",
